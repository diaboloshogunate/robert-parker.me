{% block form_errors %}
    {% spaceless %}
        {% if errors|length > 0 %}
            <ul class="no-bullet error">
                {% for error in errors %}
                    <li>{{ error.message }}</li>
                {% endfor %}
            </ul>
        {% endif %}
    {% endspaceless %}
{% endblock form_errors %}
{% block form_row %}
    <div class="form_row">
        {{ form_label(form) }}
        {{ form_widget(form) }}
    </div>
{% endblock form_row %}